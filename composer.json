{
  "name": "gudik/yii2-hydraphp",
  "description": "PHP Builder for ERP Hydra API",
  "type": "yii2-extension",
  "keywords": ["yii2", "yii 2", "hydra"],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Aleksey Klimov",
      "email": "ax.klim@gmail.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "~2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "fzaninotto/faker": "^1.7"
  },
  "autoload": {
    "psr-4": {
      "gudik\\hydraphp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "gudik\\hydraphp\\tests\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
